<p id="notice"><%= notice %></p>

<h1>Listing Course Has Homeworks</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Homework</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_has_homeworks.each do |course_has_homework| %>
      <tr>
        <td><%= course_has_homework.course %></td>
        <td><%= course_has_homework.homework %></td>
        <td><%= link_to 'Show', course_has_homework %></td>
        <td><%= link_to 'Edit', edit_course_has_homework_path(course_has_homework) %></td>
        <td><%= link_to 'Destroy', course_has_homework, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course has homework', new_course_has_homework_path %>
